.hdicon p { display:flex; align-items: center;font-family:var(--{{ settings.navft }});  font-size:var(--fs); line-height:1; font-weight:{{ settings.navfw }}; {% if settings.nav_case %} text-transform: uppercase; {% endif %} text-transform: capitalize;}

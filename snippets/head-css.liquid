.search .s_submit{ color:{{ settings.search_btnicon }}; background:#fafafa; padding:0; width:60px; border-left:0; border-radius:{% if is_rtl %}{{ settings.search_rad }}px 0 0 {{ settings.search_rad }}px{% else %}0 {{ settings.search_rad }}px {{ settings.search_rad }}px 0{% endif %}; } 

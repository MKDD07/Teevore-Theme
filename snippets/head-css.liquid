.crlgTtl:after { content:' '; -webkit-mask-size:cover; mask-size:cover; -webkit-mask-image: url({{ 'arrow.svg' | asset_url }}); mask-image: url({{ 'arrow.svg' | asset_url }});
    width:8px; height:8px; background-color:var(--cl, {{ settings.mobTtl }}); margin-{{ p_left }}:5px; display:inline-block; vertical-align:middle; }
.cnrList { top:30px; left:50%; transform:translateX(-50%); width:max-content; min-width:120px; padding:15px; max-height:400px; overflow:auto; background-color:{{ settings.ddbg }}; box-shadow:0px 5px 10px rgba(0,0,0,0.1); }
.cnrList:not(.active) { display:none; }
.cnrList li { color:{{ settings.ddtxt }}; padding:7px 5px; cursor:pointer; text-transform:capitalize; font-size:13px; }
.cnrList li:hover, .cnrList li.selected { opacity:0.6 }
.crlgTtl img { max-width:15px }
.c_code { margin-{{ p_left }}:14px; }
.c_code:before { 
  position:absolute; content:""; width:1px; height:10px; background:{{ settings.ddtxt }}; top:50%; opacity:0.4;
  {%- if is_rtl -%}
      right:-7px; margin-top:-5px;
  {%- else -%}
      left:-7px; margin-top:-5px;
  {%- endif -%}
}
  

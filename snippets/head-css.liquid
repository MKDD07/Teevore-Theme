{% comment %}theme-check-disable{% endcomment %}
{%- capture sitecss -%}
{%- liquid
    if is_rtl
        assign css_file = 'themeRtl.css'
        assign p_left = 'right'
        assign p_right = 'left'
    else
        assign css_file = 'theme.css'
        assign p_left = 'left'
        assign p_right = 'right'
    endif 
-%}<link rel="preload" as="style" href="{{ css_file | asset_url }}">
{% comment %} Font Loading Logic - Individual Source Selection {% endcomment %}

{% comment %} Collect all unique font sources {% endcomment %}
{% liquid
    assign font_sources = settings.font1_source | append: ',' | append: settings.font2_source | append: ',' | append: settings.font3_source | split: ',' | uniq
%}

{% comment %} Handle Shopify Fonts {% endcomment %}
{% if font_sources contains 'shopify' %}
    {% liquid
        assign shopify_fonts = ''
        if settings.font1_source == 'shopify'
            assign shopify_fonts = shopify_fonts | append: '1,'
        endif
        if settings.font2_source == 'shopify'
            assign shopify_fonts = shopify_fonts | append: '2,'
        endif
        if settings.font3_source == 'shopify'
            assign shopify_fonts = shopify_fonts | append: '3,'
        endif
        assign shopify_fonts = shopify_fonts | split: ',' | compact
    %}
    
    {% comment %} Preload Shopify fonts {% endcomment %}
    {% for font_num in shopify_fonts %}
        {% assign font_setting = 'font' | append: font_num | append: '_shopify' %}
        {% assign current_font = settings[font_setting] %}
        {%- for variant in current_font.variants -%}
            {% if variant.weight == 400 or variant.weight == 700 %}
               {% unless variant.style == "italic" %}<link rel="preload" as="font" href="{{ variant | font_url }}" type="font/woff2" crossorigin>{% endunless %}
            {% endif %}
        {%- endfor -%}
    {% endfor %}
{% endif %}

{% comment %} Handle Google Fonts {% endcomment %}
{% if font_sources contains 'google' %}
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    
    {% liquid
        assign google_fonts = ''
        if settings.font1_source == 'google'
            assign google_fonts = google_fonts | append: settings.font1_google | strip | replace: ' ', '+' | append: ','
        endif
        if settings.font2_source == 'google'
            assign google_fonts = google_fonts | append: settings.font2_google | strip | replace: ' ', '+' | append: ','
        endif
        if settings.font3_source == 'google'
            assign google_fonts = google_fonts | append: settings.font3_google | strip | replace: ' ', '+' | append: ','
        endif
        assign google_fonts = google_fonts | split: ',' | compact | uniq
    %}
    
    {%- capture gfonts -%}
        {%- for font in google_fonts -%}family={{ font }}:wght@300;400;500;600;700{% unless forloop.last %}&{% endunless %}{% endfor %}
    {%- endcapture -%}
    <link href="https://fonts.googleapis.com/css2?{{ gfonts }}&display=swap" rel="stylesheet" media="print" onload="this.media='all'">
{% endif %}

{% comment %} Handle Local Fonts {% endcomment %}
{% if font_sources contains 'local' %}
    {% comment %} Preload critical local font weights 
    <link rel="preload" as="font" href="https://cdn.shopify.com/s/files/1/0903/6585/3983/files/Gustavo-Regular.woff2" type="font/woff2" crossorigin>
    <link rel="preload" as="font" href="https://cdn.shopify.com/s/files/1/0903/6585/3983/files/Gustavo-Medium.woff2" type="font/woff2" crossorigin>
    <link rel="preload" as="font" href="https://cdn.shopify.com/s/files/1/0903/6585/3983/files/Gustavo-Bold.woff2" type="font/woff2" crossorigin>
    {% endcomment %}
{% endif %}

{% comment %} Generate CSS for all font sources {% endcomment %}
{% capture font_css %}
    {% comment %} Shopify Fonts CSS {% endcomment %}
    {% if font_sources contains 'shopify' %}
        {% for font_num in shopify_fonts %}
            {% assign font_setting = 'font' | append: font_num | append: '_shopify' %}
            {% assign current_font = settings[font_setting] %}
            {%- for variant in current_font.variants -%}
                {% assign weight_num = variant.weight | times: 1 %}
                {% if weight_num >= 300 and weight_num <= 700 and variant.style != "italic" %}
                    {{ variant | font_face: font_display: 'swap' }}
                {% endif %}
            {%- endfor -%}
        {% endfor %}
    {% endif %}

    {% if font_sources contains 'local' %}

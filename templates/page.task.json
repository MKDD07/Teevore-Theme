       }\n\n        .slick-next::before {\n            transform: rotate(45deg);\n            margin-right: 3px;\n        }\n\n        /* Custom Dots Styles */\n        .slick-dots {\n            display: flex !important;\n            justify-content: center;\n            align-items: center;\n            gap: 8px;\n            list-style: none;\n            padding: 0;\n            margin: 15px 0 0 0;\n        }\n\n        .slick-dots li {\n            margin: 0;\n            padding: 0;\n        }\n\n        .slick-dots li button {\n            width: 10px;\n            height: 10px;\n            border-radius: 50%;\n            background: rgba(255, 255, 255, 0.5);\n            border: none;\n            cursor: pointer;\n            font-size: 0;\n            padding: 0;\n            transition: background 0.3s ease;\n        }\n\n        .slick-dots li.slick-active button {\n            background: white;\n        }\n\n        /* CTA Button */\n        .cta-button {\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            padding: 8px 12px;\n            background: #000;\n            border: 1px solid #fff;\n            color: #fff;\n            font-size: 13px;\n            font-weight: 500;\n            text-decoration: none;\n            cursor: pointer;\n            margin: 20px auto;\n            transition: all 0.3s ease;\n        }\n\n        .cta-button:hover {\n            background: #fff;\n            color: #000;\n        }\n\n        /* Mobile Responsive */\n        @media (max-width: 576px) {\n            .section {\n                height: 100vh;\n                height: 100dvh;\n            }\n\n            .hero-video {\n                height: 300px;\n                object-position: right center;\n            }\n\n            .hero-brand {\n                font-size: 24px;\n            }\n\n            .hero-model {\n                font-size: 64px;\n            }\n\n            .image-sequence-container {\n                height: 400px;\n            }\n\n            .animated-text {\n                font-size: 24px;\n            }\n\n            .carousel-container {\n                margin: 20px auto 10px;\n                min-height: 300px;\n            }\n\n            .slide-base-image {\n                height: auto;\n                max-height: 300px;\n                object-fit: contain;\n            }\n\n            .slide-overlay img {\n                height: auto;\n                max-height: 300px;\n            }\n\n            .cta-button {\n                margin: 10px auto;\n                font-size: 14px;\n                padding: 8px 16px;\n            }\n\n            .image-controls {\n                bottom: 10px;\n                flex-wrap: wrap;\n                justify-content: center;\n            }\n\n            .image-controls button {\n                padding: 6px 12px;\n                font-size: 12px;\n            }\n        }\n\n        .slide-base-image {\n            position: relative;\n        }\n    </style>\n</head>\n\n<body>\n    <section class=\"section\" style=\"position: relative;\">\n        <span><img src=\"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/Renault_2021_Text_1.png?v=1765212013\"\n                alt=\"\" style=\"width: 60px;\n    position: absolute;\n    top: 20px;\n    z-index: 9999;\n    right: 20px;\"></span>\n        <!-- Hero Video Section -->\n        <div class=\"hero-video-container\">\n            <video class=\"hero-video\" autoplay loop muted playsinline>\n                <source\n                    src=\"https://cdn.group.renault.com/ren/in/kiger-ph2/RENAULT_KIGER_TVC_10s_INDE_2025_1280x720_VI.mp4.asset.mp4/7268a0ecc6.mp4\"\n                    type=\"video/mp4\">\n            </video>\n            <span class=\"hero-text hero-brand\">RENAULT</span>\n            <span class=\"hero-text hero-model\">KIGER</span>\n            <span class=\"gradient-overlay\"></span>\n        </div>\n\n        <!-- Image Sequence Section -->\n        <div class=\"image-sequence-container\">\n            <span class=\"animated-text\" id=\"animatedText\">Specification</span>\n            <img id=\"img1\" class=\"sequence-image\"\n                src=\"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/031313.png?v=1765207920\" alt=\"Image 1\">\n            <img id=\"img2\" class=\"sequence-image\"\n                src=\"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/0000132.png?v=1765204361\" alt=\"Image 2\">\n            <img id=\"img3\" class=\"sequence-image\"\n                src=\"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/0000132_3759361f-86ef-4ddc-a995-de754242bb29.png?v=1765205388\"\n                alt=\"Image 3\">\n            <img id=\"img4\" class=\"sequence-image\"\n                src=\"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/03131367.png?v=1765208818\" alt=\"Image 4\">\n\n            <div class=\"image-controls\" id=\"imageControls\">\n                <button onclick=\"changeImg(0)\" class=\"active cta-button\">Royal Amethyst</button>\n                <button onclick=\"changeImg(1)\" class=\"cta-button\">Neon Lime</button>\n                <button onclick=\"changeImg(2)\" class=\"cta-button\">Solar Flame</button>\n                <button onclick=\"changeImg(3)\" class=\"cta-button\">Viper Green</button>\n            </div>\n        </div>\n\n        <!-- Carousel Section -->\n        <div class=\"carousel-container\">\n            <div class=\"slick-slider\">\n                <div style=\"position: relative;margin: 0 15px;\">\n                    <img class=\"slide-base-image\"\n                        src=\"\n                        https://cdn.group.renault.com/ren/master/renault-new-cars/product-plans/kiger/ph2/overview/renault-kiger-ph2-overview-004-desktop.jpg.ximg.largex2.webp/c5f53776a4.webp\"\n                        alt=\"Slide 1\">\n                    <span class=\"slide-overlay\">\n                        <img src=\"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/Frame_224.png?v=1765208219\"\n                            alt=\"Overlay 1\">\n                    </span>\n                </div>\n                <div style=\"position: relative;margin: 0 15px;\">\n                    <img class=\"slide-base-image\"\n                        src=\"https://cdn.group.renault.com/ren/master/renault-new-cars/product-plans/kiger/ph2/overview/renault-kiger-ph2-overview-005-desktop.jpg.ximg.largex2.webp/760a262acf.webp\"\n                        alt=\"Slide 2\">\n                    <span class=\"slide-overlay\">\n                        <img src=\"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/Frame_224_505d32c0-5078-46ee-821e-3b86a98dc326.png?v=1765208324\"\n                            alt=\"Overlay 2\">\n                    </span>\n                </div>\n                <div style=\"position: relative;margin: 0 15px;\">\n                    <img class=\"slide-base-image\"\n                        src=\"https://cdn.group.renault.com/ren/master/renault-new-cars/product-plans/kiger/ph2/overview/renault-kiger-ph2-overview-003-desktop.jpg.ximg.largex2.webp/722c310033.webp\"\n                        alt=\"Slide 3\">\n                    <span class=\"slide-overlay\">\n                        <img src=\"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/Frame_223_a71923a0-da7e-42b2-990a-a97032815216.png?v=1765208324\"\n                            alt=\"Overlay 3\">\n                    </span>\n                </div>\n            </div>\n        </div>\n\n        <a class=\"cta-button\" href=\"https://adomantra.com/ads/RE_Mob1/Carousel.html\">KNOW MORE</a>\n    </section>\n\n    <script>\n        // Image sequence animation\n        const images = [\n            \"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/03131367.png?v=1765211589\",\n            \"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/031313_ad62e9a8-2e76-4ed5-a7fa-99200d5520e8.png?v=1765208818\",\n            \"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/0313135.png?v=1765208818\",\n            \"https://cdn.shopify.com/s/files/1/0781/5148/3632/files/0313136.png?v=1765208818\"\n        ];\n\n        let currentImageIndex = 0;\n\n        // Animated text sequence\n        const text = document.getElementById('animatedText');\n        setTimeout(() => {\n            text.style.opacity = '1';\n            text.style.animation = 'textFill 2s ease forwards';\n        }, 3500);\n\n        setTimeout(() => {\n            text.style.transition = 'opacity 0.5s ease';\n            text.style.opacity = '0';\n        }, 6500);\n\n        // Image sequence timeline\n        setTimeout(() => document.getElementById(\"img1\").style.opacity = \"1\", 2000);\n        setTimeout(() => {\n            document.getElementById(\"img1\").style.opacity = \"0\";\n            document.getElementById(\"img2\").style.opacity = \"1\";\n        }, 3000);\n        setTimeout(() => {\n            document.getElementById(\"img2\").style.opacity = \"0\";\n            document.getElementById(\"img3\").style.opacity = \"1\";\n        }, 6000);\n        setTimeout(() => {\n            document.getElementById(\"img3\").style.opacity = \"0\";\n            document.getElementById(\"img4\").style.opacity = \"1\";\n            document.getElementById(\"imageControls\").classList.add('visible');\n        }, 8000);\n\n        // Interactive image switcher\n        function changeImg(index) {\n            const img = document.getElementById(\"img4\");\n            const buttons = document.querySelectorAll('.image-controls button');\n\n            currentImageIndex = index;\n\n            img.style.opacity = \"0\";\n\n            buttons.forEach((btn, i) => {\n                btn.classList.toggle('active', i === index);\n            });\n\n            setTimeout(() => {\n                img.src = images[index];\n                img.style.opacity = \"1\";\n            }, 300);\n        }\n\n        // Slick carousel initialization\n        jQuery(document).ready(function ($) {\n            const $slider = $('.slick-slider');\n            const $images = $slider.find('img');\n            let imagesLoaded = 0;\n            const totalImages = $images.length;\n\n            setTimeout(() => $('.section').addClass('loaded'), 1000);\n\n            function initSlick() {\n                $slider.slick({\n                    infinite: true,\n                    speed: 600,\n                    centerMode: true,\n                    centerPadding: '60px',\n                    slidesToShow: 1,\n                    autoplay: true,\n                    autoplaySpeed: 3000,\n                    arrows: true,\n                    prevArrow: '<button type=\"button\" class=\"slick-prev\"></button>',\n                    nextArrow: '<button type=\"button\" class=\"slick-next\"></button>',\n                    appendDots: $('.carousel-container'),\n                    adaptiveHeight: false,\n                    cssEase: 'ease-in-out',\n                    waitForAnimate: false\n                });\n\n                $slider.on('beforeChange', function (e, slick, currentSlide, nextSlide) {\n                    $(slick.$slides).find('.slide-overlay').css({\n                        opacity: '0',\n                        animation: 'none'\n                    });\n                });\n\n                $slider.on('afterChange', function (e, slick, currentSlide) {\n                    const $overlay = $(slick.$slides[currentSlide]).find('.slide-overlay');\n                    void $overlay[0].offsetWidth;\n                    $overlay.css('animation', 'fadeIn 0.8s ease-in 0.3s forwards');\n                });\n\n                setTimeout(() => {\n                    const slick = $slider.slick('getSlick');\n                    $(slick.$slides).find('.slide-overlay').css({\n                        opacity: '0',\n                        animation: 'none'\n                    });\n                    $(slick.$slides[0]).find('.slide-overlay').css('animation', 'fadeIn 0.8s ease-in 0.5s forwards');\n                }, 50);\n            }\n\n            function imageLoaded() {\n                imagesLoaded++;\n                if (imagesLoaded === totalImages) {\n                    initSlick();\n                }\n            }\n\n            $images.each(function () {\n                if (this.complete) {\n                    imageLoaded();\n                } else {\n                    $(this).on('load', imageLoaded).on('error', imageLoaded);\n                }\n            });\n\n            setTimeout(() => {\n                if (!$slider.hasClass('slick-initialized')) {\n                    initSlick();\n                }\n            }, 3000);\n        });\n\n        // Mobile height adjustment\n        function applyMobileHeight() {\n            const sections = document.querySelectorAll('.section');\n            sections.forEach(sec => {\n                if (window.innerWidth <= 576) {\n                    sec.style.height = window.innerHeight + \"px\";\n                } else {\n                    sec.style.height = \"\";\n                }\n            });\n        }\n\n        applyMobileHeight();\n        window.addEventListener(\"resize\", applyMobileHeight);\n        window.addEventListener(\"orientationchange\", () => {\n            setTimeout(applyMobileHeight, 100);\n        });\n    </script>\n</body>\n\n</html>",

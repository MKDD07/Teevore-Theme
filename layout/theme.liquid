    {%- render 'svg-icons' -%}
    {%- render 'site-bottom', content_for_layout:content_for_layout, is_rtl:is_rtl -%}
<script>
(function() {
  'use strict';
  
  // Only run if not in theme editor
  if (typeof Shopify === 'undefined' || !Shopify.designMode) {
    // Check if we should perform the redirect
    const shouldRedirect = !sessionStorage.getItem('urlReplaced') && 
                          window.location.pathname === '/collections/women';
    
    if (shouldRedirect) {
      try {
        // Replace the URL without reloading
        window.history.replaceState(null, '', '/');
        
        // Mark that we have replaced URL this session
        sessionStorage.setItem('urlReplaced', 'true');
        
        // Optional: Update page title if needed
        // document.title = 'Your Home Page Title';
        
      } catch (error) {
        console.warn('URL replacement failed:', error);
      }
    }
  }
})();
</script>

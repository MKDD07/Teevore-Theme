{% assign section_st = section.settings %}
<div
  class="faq-cc sec_cov{% if section_st.hidem %} hide-sm{% endif %}{% if section_st.hided %} hide-md hide-lg{% endif %}{% if section_st.bgimg != blank %} bgImg lazyload{% endif %}"
  style="{% unless request.path contains '/collections/women' or template contains  'girls_collections' %}--sbg:{% if section_st.bgg != blank %}{{ section_st.bgg }}{% else %}{{ section_st.bg }};{% endif %};{% else %}--sbg:#f1f1f1;{% endunless %}--sst:{{ section_st.sst }}px;--ssb:{{ section_st.ssb }}px;--sstm:{{ section_st.sstm }}px;--ssbm:{{ section_st.ssbm }}px;--overlay:{{ section_st.overlay | color_modify: 'alpha', section_st.overlay_op }};"
  {% if section_st.bgimg != blank %}
    data-bgset="{{ section_st.bgimg | image_url: width: section_st.bgimg.width | image_tag: widths: '400,600,800,1000,1400,1600,2000,2400,3000' | split: 'srcset="' | last | split: '"' | first }}"
    data-sizes="auto" data-parent-fit="contain"
  {% endif %}
>
  <div class="{% if section_st.sw %}fw-sec fl f-col f-row-md{% else %}page-width{% endif %}{% unless section_st.enable_small_card %}wd100{% endunless %}">
    {% render 'sec-title' %}

    {% liquid
      assign is_links = false
     assign has_newsletter = false
     assign has_contact_form = false
      for block in section.blocks
        if block.type == 'ttl' or block.type == 'link'
          if section_st.links
            assign is_links = true
          endif
        elsif block.type == 'newsletter'
          assign has_newsletter = true
       elsif block.type == 'contact_form'
          assign has_contact_form = true
        endif
      endfor
    %}

    {%- if is_links -%}
      <div class="fl f-ais gap f-col f-row-md" style="--gap:50px;--gapm:20px;">
        <div
          class="wd40 faqLinks {% render 'animate-data', css:true %}"
          style="--cl:{{ section_st.lcl }};--clh:{{ section_st.lclh }};--fs:{{ section_st.lfs }}px;--fsm:{{ section_st.lfsm }}px;--fw:{{ section_st.lfw }};--bg:{{ section_st.lbg }};--bd:{{ section_st.lbd }};--icns:14px;"
        >
          {% for block in section.blocks %}
            {% assign blk_st = block.settings %}
            {% if block.type == 'ttl' or block.type == 'link' %}
              <a
                href="{% if block.type == 'link' %}{{ block.settings.link }}{% else %}#tt{{ block.id }}{% endif %}"
                class="rgTxt fl f-aic f-jcsb"
              >
                {{ block.settings.title }}
                <svg class="at-icon" viewBox="0 0 24 24" xmlns="https://www.w3.org/2000/svg">
                  <path d="m19.389 11.123c-.331 0-.6.269-.6.6v6.222c0 1.63-1.326 2.956-2.956 2.956h-9.777c-1.63 0-2.956-1.326-2.956-2.956v-9.777c0-1.63 1.326-2.956 2.956-2.956h6.222c.332 0 .6-.269.6-.6s-.269-.6-.6-.6h-6.222c-2.292 0-4.156 1.864-4.156 4.155v9.777c0 2.291 1.864 4.155 4.156 4.155h9.777c2.291 0 4.155-1.864 4.155-4.155v-6.222c0-.331-.269-.6-.6-.6z"/>
                  <path d="m21.5 1.9h-4.802c-.331 0-.6.269-.6.6s.269.6.6.6h3.354l-7.672 7.671c-.234.233-.234.614 0 .848.117.117.271.176.424.176s.307-.059.424-.176l7.671-7.671v3.384c0 .331.269.6.6.6s.6-.269.6-.6v-4.832c0-.331-.269-.6-.6-.6z"/>
                </svg>
              </a>
            {% endif %}
          {% endfor %}
        </div>
    {%- endif -%}

    <div class="{% if section_st.enable_small_card %}resp-flex col-md-2 col-1{% else %}col-1 {% endif %}">
      <div
        id="faq{{ section.id }}"
        class="{% if is_links %}fl1{% endif %}{% if section_st.accor %} accor{% else %} no-accor{% endif %}{% if has_newsletter or has_contact_form %} wd100 wd60-md{% else %} col-100{% endif %}"
        style="width: stretch; max-width:{{ section_st.page_width }}px;--cl:{% unless request.path contains '/collections/women' or template contains  'girls_collections' %} {{ section_st.an }};{% else %} #222;{% endunless %} --fs:{{ section_st.fst }}px;--fsm:{{ section_st.fsmt }}px;--qcl:{{ section_st.qu }};--qfs:{{ section_st.fs }}px;--qfsm:{{ section_st.fsm }}px;--qfw:{{ section_st.fw }};--bdr:{{ section_st.bdr }};"
      >
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'spacing' %}
            <div style="margin-bottom: {{ block.settings.mbb }}px; margin-top: {{ block.settings.mtt }}px; {% if block_st.enable_bb %}border-bottom: 1px solid var(--bdr);{% endif %}"></div>
            {% when 'ttl' %}
              <div class="faq-title fl gap10 f-acc" id="tt{{ block.id }}" {{ block.shopify_attributes }}>
                <span style="margin-right; 10px">{{block.settings.content_svg }}</span>
                <h2 style="color:{{ section_st.tcl }};font-size:{{ section_st.tfs }}px;font-weight:{{ section_st.tfw }};font-family:'{{settings.font_face_family_sec }}';">
                  {{ block.settings.title }}
                </h2>
              </div>
            {% when 'faqs' %}
              <div class="faq-item{% if block.settings.open %} open{% endif %}" {{ block.shopify_attributes }}>
                {% if section_st.accor %}
                  <div class="faq-question" onclick="toggleFAQ(this)">
                    <h3 class="rgTxt mb5 ft1 fwsb {% render 'animate-data', css:true, text:true %}" style="color:">
                      {{ block.settings.title }}
                    </h3>
                    <span class="faq-icon">
                      {% if section_st.acc_style == '2' %}
                        <svg class="svg" width="20" height="20" viewBox="0 0 20 20"
                            xmlns="https://www.w3.org/2000/svg"
                            fill="{% if section.settings.icon_color != blank %}{{ section.settings.icon_color }}{% else %}currentColor{% endif %}">
                          <path d="M10 15l-5-5h10l-5 5z" />
                        </svg>
                      {% else %}
                        <span class="plus-minus">+</span>
                      {% endif %}
                    </span>
                  </div>
                  <div
                    class="faq-answer"
                    {% if block.settings.open %}
                      style="display:block;"
                    {% endif %}
                  >
                    {{ block.settings.answer }}
                  </div>
                {% else %}
                  <div class="faq-question">
                    <h3 class="rgTxt mb5 ft2 fwm {% render 'animate-data', css:true, text:true %}">
                      >{{ block.settings.title }}
                    </h3>
                  </div>
                  <div class="faq-answer" style="display:block;">
                    {{ block.settings.answer }}
                  </div>
                {% endif %}
              </div>
          {% endcase %}
        {% endfor %}
      </div>

      {% if has_newsletter or has_contact_form %}
        <div class="wd100 wd40-md hide-md hide-sm">
          {% for block in section.blocks %}
            {% assign blk_st = block.settings %}
            {% if block.type == 'newsletter' %}
              <div {{ block.shopify_attributes }}>
                {% render 'news-letter-snippet', block: block %}
              </div>
                 {% elsif block.type == 'contact_form' %}
                 <div {{ block.shopify_attributes }}>
          		<div class="contact-page sec_cov{% if blk_st.hidem %} hide-sm{% endif %}{% if blk_st.hided %} hide-md hide-lg{% endif %}{% if blk_st.bgimg != blank %} bgImg lazyload{% endif %}" style="--sbg:{% if blk_st.bgg != blank %}{{ blk_st.bgg }}{% else %}{{ blk_st.bg }}{% endif %};--sst:{{ blk_st.sst }}px;--ssb:{{ blk_st.ssb }}px;--sstm:{{ blk_st.sstm }}px;--ssbm:{{ blk_st.ssbm }}px;--cl:{{ blk_st.cl }};--cl:{{blk_st.headings_cl }};--overlay:{{ blk_st.overlay | color_modify: 'alpha', blk_st.overlay_op }};"
   {% if blk_st.bgimg != blank %}data-bgset="{{ blk_st.bgimg | image_url: width: blk_st.bgimg.width | image_tag: widths: '400,600,800,1000,1400,1600,2000,2400,3000' | split: 'srcset="' | last | split: '"' | first }}" data-sizes="auto" data-parent-fit="contain"{% endif %}>  	
  <div class="page-width" style="--page-width:{{ blk_st.page_width }}px;">        
      {% if page.content.size > 0 %}{{ page.content }}{% endif %}    

      {% if blk_st.title != blank %}<div class="mb10 sec-ttl tc" style="padding-bottom:0 !important">{{ blk_st.title }}</div>{% endif %}
      {% if blk_st.details %}<div class="rte tc mb30">{{ blk_st.details }}</div>{% endif %}
    
      <div class="grid contact{{ blk_st.layout }}{% if blk_st.layout == "2" %} f-wrap{% endif %}">
        
        <div class="gitem{% if blk_st.layout == "1" %} wd66-md{% endif %}">          
          <div class="contact-frm form-vertical {% render 'animate-data', css:true %}">
            {% form 'contact' %}
            {% if form.posted_successfully? %}<p class="border form-success">{{ 'contact.form.post_success' | t }}</p>{% endif %}

            {{ form.errors | default_errors }}
            {% # theme-check-disable UndefinedObject %}
                <div class="fl f-col f-row-md col-gap" style="--gap:15px;">
                  <div class="fl1 f-col f-row-md">
                    <input type="text" id="ContactFormName" name="contact[name]" placeholder="{{ 'contact.form.name' | t }}" value="{% if form[name] %}{{ form[name] }}{% elsif customer %}{{ customer.name }}{% endif %}">
                  </div>       
                  <div class="fl1">
                    <input type="email" id="ContactFormEmail" name="contact[email]" placeholder="{{ 'contact.form.email' | t }}" autocorrect="off" autocapitalize="off" value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}" class="{% if form.errors contains 'email' %}input--error{% endif %}">
                  </div>              
                </div>
                {% unless blk_st.show_phone %}
                <input type="tel" id="ContactFormPhone" name="contact[phone]" pattern="[0-9\-]*" placeholder="{{ 'contact.form.phone' | t }}" value="{% if form[phone] %}{{ form[phone] }}{% elsif customer %}{{ customer.phone }}{% endif %}">
                {% endunless %} 
                {% unless blk_st.show_subject %}
                <input type="text" id="ContactFormSubject" name="contact[email subject]" placeholder="{{ 'contact.form.subject' | t }}" value="{% if form[subject] %}{{ form[subject] }}{% endif %}">
                {% endunless %}
                <textarea rows="10" id="ContactFormMessage" name="contact[body]" placeholder="{{ 'contact.form.message' | t }}">{% if form.body %}{{ form.body }}{% endif %}</textarea>
                <input type="submit" class="btn {{ blk_st.btn-style }}" value="{{ 'contact.form.submit' | t }}">
            {% # theme-check-enable UndefinedObject %}
            {% endform %}
          </div>        
        </div>    
      </div>       
  </div>
</div>
	</div>
            {% endif %}
          {% endfor %}
        </div>
      {% endif %}
    </div>

    {%- if is_links -%}
      </div>
    {%- endif -%}
  </div>
</div>

<script>
  function toggleFAQ(element) {
    const faqItem = element.closest('.faq-item');
    const answer = faqItem.querySelector('.faq-answer');
    const icon = element.querySelector('.plus-minus');

    if (answer.style.display === 'block') {
      answer.style.display = 'none';
      faqItem.classList.remove('open');
      if (icon) icon.textContent = '+';
    } else {
      answer.style.display = 'block';
      faqItem.classList.add('open');
      if (icon) icon.textContent = '-';
    }
  }
</script>

<style>
  .faq-item {
    border-bottom: 1px solid {% unless request.path contains '/collections/women' or template contains 'girls_collections' %}
        var(--bdr)
    {% else %}
       #d9d9d9
    {% endunless %};
    padding: 15px 0;
  }
  .faq-cc .em_fancy {
      -webkit-text-stroke: 1px {{section_st.tcl}};
      -webkit-text-fill-color: transparent;}
.faq-cc .sec-ttl .sbttl {
    color: {% unless request.path contains '/collections/women' or template contains 'girls_collections' %}
        {{ section_st.tcl }}
    {% else %}
       #222 
    {% endunless %};
}

.faq-question {
    cursor: pointer;
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: {% unless request.path contains '/collections/women' or template contains 'girls_collections' %}var(--qcl){% else %}#222{% endunless %};
    font-size: var(--qfs);
    font-weight: var(--qfw);
}


  .faq-answer {
    display: none;
    padding-top: 15px;
    color: var(--cl);
    font-size: var(--fs);
  }

  .no-accor .faq-question {
    cursor: default;
  }

  .no-accor .faq-answer {
    display: block !important;
  }

  .faq-icon {
    font-size: 18px;
    font-weight: bold;
  }

  .faq-title h3 {
    margin: 20px 0 10px 0;
  }

  @media (max-width: 768px) {
    .faq-question {
      font-size: var(--qfsm);
    }

    .faq-answer {
      font-size: var(--fsm);
    }

    .faq-title h3 {
      font-size: var(--tfsm) !important;
    }
  }
</style>

